{
  "name": "figma-html-converter",
  "version": "1.0.0",
  "description": "A Figma plugin that converts HTML into Figma designs",
  "main": "dist/code.js",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "test": "vitest --no-watch",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "figma",
    "plugin",
    "html",
    "converter"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@figma/plugin-typings": "^1.90.0",
    "@mizchi/lsmcp": "^0.9.4",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "@typescript/native-preview": "^7.0.0-dev.20250825.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.2.6",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
